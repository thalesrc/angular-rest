{
  "name": "angular-rest-app",
  "version": "0.0.0",
  "description": "Angular HTTP client with Typescript Declarative Annotations, Guards, Handlers and more",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build rest",
    "build:playground": "npm run build && ng build playground --prod",
    "test": "ng test rest",
    "test:travis": "ng test rest --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copy:license": "cpy 'LICENSE' './dist/rest'",
    "copy:readme": "cpy 'README.md' './dist/rest'",
    "copy:files": "npm run copy:license && npm run copy:readme",
    "pack:library": "cd dist/rest && npm pack",
    "prepare:tag-update": "node ./scripts/tag-update $TRAVIS_TAG",
    "prepare:npm-config": "node ./scripts/npm-config $NPM_EMAIL $NPM_TOKEN",
    "prepare:package": "npm run build && npm run copy:files && npm run prepare:tag-update && npm run prepare:npm-config && npm run pack:library",
    "publish:package": "npm publish ./dist/rest/thalesrc-angular-rest-$TRAVIS_TAG.tgz",
    "preapare-and-publish": "npm run prepare:package && npm run publish:package",
    "document": "compodoc -p projects/rest/tsconfig.lib.json --theme material --disablePrivate && echo \"\" > ./documentation/.nojekyll"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.3",
    "@angular/common": "~8.2.3",
    "@angular/compiler": "~8.2.3",
    "@angular/core": "~8.2.3",
    "@angular/forms": "~8.2.3",
    "@angular/platform-browser": "~8.2.3",
    "@angular/platform-browser-dynamic": "~8.2.3",
    "@angular/router": "~8.2.3",
    "core-js": "^2.5.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.5.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.1",
    "@angular-devkit/build-ng-packagr": "~0.803.1",
    "@angular/cli": "~8.3.1",
    "@angular/compiler-cli": "~8.2.3",
    "@angular/language-service": "~8.2.3",
    "@compodoc/compodoc": "^1.1.10",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "cpy-cli": "^2.0.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^5.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.0",
    "tslib": "^1.10.0",
    "tslint": "~5.11.0",
    "typescript": "~3.5.3",
    "update-json": "^1.0.0"
  }
}
